<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:orientation="vertical">

            <TextView
                style="@style/TextViewStyle1"
                android:text="1.1 企业资质"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="5dp"
                android:background="#F5F5F5"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#fff"
                android:orientation="vertical"
                android:padding="10dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        style="@style/TextViewStyle2"
                        android:text="@string/enterprise_qualification1.1.1"/>

                    <RadioGroup
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <RadioButton
                            android:id="@+id/rb_yes1_1_1"
                            style="@style/TextStyle"
                            android:text="Y"/>

                        <RadioButton
                            android:id="@+id/rb_no1_1_1"
                            style="@style/TextStyle"
                            android:text="N"
                            android:checked="true"/>
                    </RadioGroup>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        style="@style/TextStyle"
                        android:text="居住地："/>

                    <EditText
                        android:id="@+id/et_address1_1_1"
                        android:layout_width="400dp"
                        style="@style/EditTextStyle"/>
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="5dp"
                android:background="#F5F5F5"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#fff"
                android:orientation="vertical"
                android:padding="10dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        style="@style/TextViewStyle2"
                        android:text="@string/enterprise_qualification1.1.2"/>

                    <RadioGroup
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <RadioButton
                            android:id="@+id/rb_yes1_1_2"
                            style="@style/TextStyle"
                            android:text="Y"/>

                        <RadioButton
                            android:id="@+id/rb_no1_1_2"
                            style="@style/TextStyle"
                            android:text="N"
                            android:checked="true"/>
                    </RadioGroup>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/TextStyle"
                        android:text="实际："/>

                    <EditText
                        android:id="@+id/et_money1_1_2"
                        android:layout_width="100dp"
                        android:inputType="number"
                        style="@style/EditTextStyle"/>

                    <TextView
                        style="@style/TextStyle"
                        android:text="万元"
                        android:layout_marginLeft="5dp"/>
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="5dp"
                android:background="#F5F5F5"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#fff"
                android:orientation="vertical"
                android:padding="10dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        style="@style/TextViewStyle2"
                        android:text="@string/enterprise_qualification1.1.3"/>

                    <RadioGroup
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <RadioButton
                            android:id="@+id/rb_yes1_1_3"
                            style="@style/TextStyle"
                            android:text="Y"/>

                        <RadioButton
                            android:id="@+id/rb_no1_1_3"
                            style="@style/TextStyle"
                            android:text="N"
                            android:checked="true"/>
                    </RadioGroup>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        style="@style/TextStyle"
                        android:text="注册证号："/>

                    <EditText
                        android:id="@+id/et_code_number1_1_3"
                        android:layout_width="200dp"
                        style="@style/EditTextStyle"/>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#fff"
                android:orientation="vertical"
                android:padding="10dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        style="@style/TextViewStyle2"
                        android:text="@string/enterprise_qualification1.1.4"/>

                    <RadioGroup
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <RadioButton
                            android:id="@+id/rb_yes1_1_4"
                            style="@style/TextStyle"
                            android:text="Y"/>

                        <RadioButton
                            android:id="@+id/rb_no1_1_4"
                            style="@style/TextStyle"
                            android:text="N"
                            android:checked="true"/>
                    </RadioGroup>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <CheckBox
                        android:id="@+id/cb_first_1_1_4"
                        style="@style/TextStyle"
                        android:text="首次"
                        android:layout_marginRight="10dp"/>
                    <TextView
                        style="@style/TextStyle"
                        android:text="　　证号："/>

                    <EditText
                        android:id="@+id/et_code_number1_1_4"
                        android:layout_width="200dp"
                        style="@style/EditTextStyle"
                        android:layout_marginRight="10dp"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <CheckBox
                        android:id="@+id/cb_review_1_1_4"
                        style="@style/TextStyle"
                        android:text="复查"
                        android:layout_marginRight="10dp"/>


                    <TextView
                        style="@style/TextStyle"
                        android:text="颁证时间："
                        android:layout_gravity="center"/>

                    <LinearLayout
                        android:id="@+id/ll_date_1_2_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="center"
                        android:background="@drawable/style_edittext">

                        <TextView
                            android:id="@+id/tv_date_1_2_1"
                            style="@style/Text"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"/>

                        <View
                            android:layout_width="2dp"
                            android:layout_height="match_parent"
                            android:background="#F5F5F5"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"/>

                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:background="@drawable/icon_date"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
    </ScrollView>

    <View
        android:id="@+id/linearLayout5"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#F5F5F5"/>

    <LinearLayout
        android:id="@+id/enterprise_qualification_bottom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#fff">

        <include
            layout="@layout/include_bottom2"/>
    </LinearLayout>

</LinearLayout>