<?xml version="1.0" encoding="utf-8"?>
<TableLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/item_detail"
    android:visibility="visible"
    android:background="@color/dialog_bg_light_blue"
    android:padding="10dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:stretchColumns="1,3,5">
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>

    <TableRow>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
        <!--********************************************************-->
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:background="@color/dialog_title"
            android:gravity="center"
            android:text="委托单编号"
            android:textColor="#000"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>

        <EditText
            android:id="@+id/et_doc_no"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="1dp"
            android:layout_weight="2"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>


    </TableRow>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>

    <TableRow>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
        <!--********************************************************-->
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:background="@color/dialog_title"
            android:gravity="center"
            android:text="委托单位"
            android:textColor="#000"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>

        <EditText
            android:id="@+id/et_project_unit"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="1dp"
            android:layout_weight="2"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>


    </TableRow>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>

    <TableRow>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
        <!--********************************************************-->
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:background="@color/dialog_title"
            android:gravity="center"
            android:text="样品来源"
            android:textColor="#000"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>

        <Spinner
            android:id="@+id/et_project_source"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="1dp"
            android:layout_weight="2"
            android:entries="@array/test_side"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>


    </TableRow>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>

    <TableRow>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
        <!--********************************************************-->
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:background="@color/dialog_title"
            android:gravity="center"
            android:text="项目名称"
            android:textColor="#000"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>

        <EditText
            android:id="@+id/et_project_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="1dp"
            android:layout_weight="2"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>


    </TableRow>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>
    <TableRow>
        <!--********************************************************-->
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:background="@color/dialog_title"
            android:gravity="center"
            android:text="样品地址"
            android:textColor="#000"
            android:textSize="@dimen/text_size_12"/>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
        <EditText
            android:id="@+id/et_project_address"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="1dp"
            android:layout_weight="2"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
    </TableRow>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>
    <TableRow>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:background="@color/dialog_title"
            android:gravity="center"
            android:text="样品区域"
            android:textColor="#000"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>

        <Spinner
            android:id="@+id/et_project_area"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="1dp"
            android:layout_weight="2"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>


    </TableRow>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>
    <TableRow>
        <!--********************************************************-->
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:background="@color/dialog_title"
            android:gravity="center"
            android:text="委托人"
            android:textColor="#000"
            android:textSize="@dimen/text_size_12"/>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>

        <EditText
            android:id="@+id/et_project_consignor"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="1dp"
            android:layout_weight="2"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
    </TableRow>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>
    <TableRow>
        <!--********************************************************-->
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:background="@color/dialog_title"
            android:gravity="center"
            android:text="收样人"
            android:textColor="#000"
            android:textSize="@dimen/text_size_12"/>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>

        <EditText
            android:id="@+id/et_project_assignee"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="1dp"
            android:layout_weight="2"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
    </TableRow>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>
    <TableRow>
        <!--********************************************************-->
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:background="@color/dialog_title"
            android:gravity="center"
            android:text="收样日期"
            android:textColor="#000"
            android:textSize="@dimen/text_size_12"/>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>

        <EditText
            android:id="@+id/et_project_date"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="1dp"
            android:layout_weight="2"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
    </TableRow>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>
    <TableRow>

        <LinearLayout
            android:layout_span="9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="right"
            >
            <Button
                android:id="@+id/submit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:layout_margin="1dp"
                android:layout_span="1"
                android:gravity="center"
                android:minHeight="0dp"
                android:text="确认"
                android:textSize="@dimen/text_size_12"/>
            <Button
                android:id="@+id/cancel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:layout_margin="1dp"
                android:layout_span="1"
                android:gravity="center"
                android:minHeight="0dp"
                android:text="取消"
                android:textSize="@dimen/text_size_12"/>


        </LinearLayout>
    </TableRow>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>
</TableLayout>