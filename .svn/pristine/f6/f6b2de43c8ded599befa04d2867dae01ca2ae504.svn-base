<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp">

        <TextView
            android:id="@+id/tv_page_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:text="样品信息-维护"
            android:textSize="14sp" />


        <TextView
            android:id="@+id/tv_report"
            android:layout_width="70dp"
            android:layout_height="30dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="80dp"
            android:gravity="center"
            android:text="检测报告" />

        <TextView
            android:id="@+id/tv_record"
            android:layout_width="70dp"
            android:layout_height="30dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="8dp"
            android:gravity="center"
            android:text="原始记录" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#000000"></View>

<ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="20dp"
            android:paddingTop="10dp">

            <TextView
                android:id="@+id/tv_1_1"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:gravity="right"
                android:text="制安企业 : "
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_company"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@id/tv_1_1"
                android:text=""
                android:textSize="14sp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="20dp"
            android:paddingTop="10dp">

            <TextView
                android:id="@+id/tv_2_1"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="项目名称 : "
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_addr"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@id/tv_2_1"
                android:text=""
                android:textSize="14sp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="20dp"
            android:paddingTop="10dp">

            <TextView
                android:id="@+id/tv_4_1"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="1dp"
                android:gravity="right"
                android:text="样品型号 : "
                android:textSize="14sp" />

            <EditText
                android:id="@+id/tv_samplemodel"
                style="@style/InputValue"
                android:background="@null"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="30dp"
                android:gravity="left"
                android:padding="2dp"
                android:text=""
                android:textSize="14sp"
                android:enabled="false"
                android:layout_alignTop="@+id/tv_4_1"
                android:layout_toEndOf="@+id/tv_4_1"
                android:layout_marginStart="10dp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="20dp"
            android:paddingTop="10dp">

            <TextView
                android:id="@+id/tv_3_1"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:gravity="right"
                android:text="样品类型 : "
                android:textSize="14sp" />

            <Spinner
                android:id="@+id/sp_type"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="30dp"
                android:layout_toRightOf="@id/tv_3_1"
                android:clickable="false"
                android:entries="@array/sample_type"
                android:background="@null"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="20dp"
            android:paddingTop="10dp">

            <TextView
                android:id="@+id/tv_14_1"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="1dp"
                android:gravity="right"
                android:text="主检人员 : "
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_mainuser"
                style="@style/InputValue"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="30dp"
                android:layout_toRightOf="@id/tv_14_1"
                android:gravity="left"
                android:padding="2dp"
                android:text=""
                android:textSize="14sp"
                android:background="@null"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="20dp"
            android:paddingTop="10dp">

            <TextView
                android:id="@+id/tv_16_1"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="1dp"
                android:gravity="right"
                android:text="辅检人员 : "
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_fuuser"
                style="@style/InputValue"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="30dp"
                android:layout_toRightOf="@id/tv_16_1"
                android:gravity="left"
                android:padding="2dp"
                android:text=""
                android:textSize="14sp"
                android:background="@null" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="20dp"
            android:paddingTop="10dp">

            <TextView
                android:id="@+id/tv_5_1"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="1dp"
                android:gravity="right"
                android:text="生产编号 : "
                android:textSize="14sp" />

            <EditText
                android:id="@+id/tv_madeno"
                style="@style/InputValue"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="30dp"
                android:layout_toRightOf="@id/tv_5_1"
                android:gravity="left"
                android:padding="2dp"
                android:text=""
                android:textSize="14sp"
                android:enabled="false"
                android:background="@null"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="20dp"
            android:paddingTop="10dp">

            <TextView
                android:id="@+id/tv_9_1"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="1dp"
                android:gravity="right"
                android:text="样品编号 : "
                android:textSize="14sp" />

            <EditText
                android:id="@+id/tv_sampleno"
                style="@style/InputValue"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="30dp"
                android:layout_toRightOf="@id/tv_9_1"
                android:gravity="left"
                android:padding="2dp"
                android:text=""
                android:textSize="14sp"
                android:enabled="false"
                android:background="@null"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="20dp"
            android:paddingTop="10dp">

            <TextView
                android:id="@+id/tv_6_1"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="1dp"
                android:gravity="right"
                android:text="制作日期 : "
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_madedate"
                style="@style/InputValue"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="30dp"
                android:layout_toRightOf="@id/tv_6_1"
                android:gravity="left"
                android:padding="2dp"
                android:text=""
                android:textSize="14sp"
                android:enabled="false"
                android:background="@null"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="20dp"
            android:paddingTop="10dp">

            <TextView
                android:id="@+id/tv_7_1"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="1dp"
                android:gravity="right"
                android:text="安装位置 : "
                android:textSize="14sp" />

            <EditText
                android:id="@+id/tv_installaddr"
                style="@style/InputValue"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="30dp"
                android:layout_toRightOf="@id/tv_7_1"
                android:gravity="left"
                android:padding="2dp"
                android:text=""
                android:textSize="14sp"
                android:enabled="false"
                android:background="@null"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="20dp"
            android:paddingTop="10dp">

            <TextView
                android:id="@+id/tv_8_1"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="1dp"
                android:gravity="right"
                android:text="安装日期 : "
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_installdate"
                style="@style/InputValue"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="30dp"
                android:layout_toRightOf="@id/tv_8_1"
                android:gravity="left"
                android:padding="2dp"
                android:text=""
                android:textSize="14sp"
                android:enabled="false"
                android:background="@null"/>
        </RelativeLayout>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="20dp"
            android:paddingTop="10dp">

            <TextView
                android:id="@+id/tv_10_1"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="1dp"
                android:gravity="right"
                android:text="检验日期 : "
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_checkdate"
                style="@style/InputValue"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="30dp"
                android:layout_toRightOf="@id/tv_10_1"
                android:gravity="left"
                android:padding="2dp"
                android:text=""
                android:textSize="14sp"
                android:enabled="false"
                android:background="@null"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="20dp"
            android:paddingTop="10dp">

            <TextView
                android:id="@+id/tv_11_1"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="1dp"
                android:gravity="right"
                android:text="温度 : "
                android:textSize="14sp" />

            <EditText
                android:id="@+id/tv_temperature"
                style="@style/InputValue"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="30dp"
                android:layout_toRightOf="@id/tv_11_1"
                android:gravity="left"
                android:padding="2dp"
                android:text=""
                android:textSize="14sp"
                android:enabled="false"
                android:background="@null"/>
        </RelativeLayout>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="20dp"
            android:paddingTop="10dp">

            <TextView
                android:id="@+id/tv_12_1"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="1dp"
                android:gravity="right"
                android:text="湿度 : "
                android:textSize="14sp" />

            <EditText
                android:id="@+id/tv_humidity"
                style="@style/InputValue"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="30dp"
                android:layout_toRightOf="@id/tv_12_1"
                android:gravity="left"
                android:padding="2dp"
                android:text=""
                android:textSize="14sp"
                android:enabled="false"
                android:background="@null"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="20dp"
            android:paddingTop="10dp">

            <TextView
                android:id="@+id/tv_13_1"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="1dp"
                android:gravity="right"
                android:text="备注说明 : "
                android:textSize="14sp" />

            <EditText
                android:id="@+id/tv_desc"
                style="@style/InputValue"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="30dp"
                android:layout_toRightOf="@id/tv_13_1"
                android:gravity="left"
                android:padding="2dp"
                android:text=""
                android:textSize="14sp"
                android:enabled="false"
                android:background="@null"/>
        </RelativeLayout>
    </LinearLayout>
</ScrollView>

</LinearLayout>