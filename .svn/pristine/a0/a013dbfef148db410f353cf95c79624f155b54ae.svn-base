<?xml version="1.0" encoding="utf-8"?>
<TableLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/item_detail"
    android:visibility="visible"
    android:background="@color/dialog_bg_light_blue"
    android:padding="10dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:stretchColumns="1,3,5">

    <TextView
        android:id="@+id/tv_project_name"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:layout_gravity="right"
        android:background="@color/project_bg_green"
        android:gravity="center"
        android:text=""
        android:textColor="#000"
        android:textSize="@dimen/text_size_18"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>

    <TableRow>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
        <!--********************************************************-->
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:background="@color/dialog_title"
            android:gravity="center"
            android:text="样品编号"
            android:textColor="#000"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>

        <EditText
            android:id="@+id/et_sample_no"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="1dp"
            android:layout_weight="2"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>


    </TableRow>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>

    <TableRow>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
        <!--********************************************************-->
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:background="@color/dialog_title"
            android:gravity="center"
            android:text="使用部位"
            android:textColor="#000"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>

        <EditText
            android:id="@+id/et_sample_location"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="1dp"
            android:layout_weight="2"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>


    </TableRow>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>
    <TableRow>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
        <!--********************************************************-->
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:textColor="#000"
            android:background="@color/dialog_title"
            android:gravity="center"
            android:text="样品类型"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>

        <Spinner
            android:id="@+id/sp_sample_type"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="1dp"
            android:layout_weight="2"
            android:entries="@array/sample_type"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>


    </TableRow>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>
    <TableRow>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
        <!--********************************************************-->
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:textColor="#000"
            android:background="@color/dialog_title"
            android:gravity="center"
            android:text="样品状态"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>

        <Spinner

            android:id="@+id/sp_sample__state"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="1dp"
            android:layout_weight="2"
            android:entries="@array/sample_state"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>


    </TableRow>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>

    <TableRow>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
        <!--********************************************************-->
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:background="@color/dialog_title"
            android:gravity="center"
            android:textColor="#000"
            android:text="样品型号"
            android:textSize="@dimen/text_size_12"/>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
        <EditText
            android:id="@+id/et_sample__model"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="1dp"
            android:layout_weight="2"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
    </TableRow>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>

    <TableRow>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
        <!--********************************************************-->
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:background="@color/dialog_title"
            android:gravity="center"
            android:textColor="#000"
            android:text="同类数量"
            android:textSize="@dimen/text_size_12"/>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
        <EditText
            android:id="@+id/et_sample_count"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="1dp"
            android:layout_weight="2"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
    </TableRow>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>
    <TableRow>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
        <!--********************************************************-->
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:background="@color/dialog_title"
            android:gravity="center"
            android:text="制作日期"
            android:textColor="#000"
            android:textSize="@dimen/text_size_12"/>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
        <EditText
            android:id="@+id/et_sample_make_date"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="1dp"
            android:layout_weight="2"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
    </TableRow>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>
    <TableRow>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
        <!--********************************************************-->
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:background="@color/dialog_title"
            android:gravity="center"
            android:text="安装日期"
            android:textColor="#000"
            android:textSize="@dimen/text_size_12"/>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
        <EditText
            android:id="@+id/et_sample_install_date"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="1dp"
            android:layout_weight="2"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
    </TableRow>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>
    <TableRow>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
        <!--********************************************************-->
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:background="@color/dialog_title"
            android:gravity="center"
            android:text="说明"
            android:textColor="#000"
            android:textSize="@dimen/text_size_12"/>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
        <EditText
            android:id="@+id/et_sample_description"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="1dp"
            android:layout_weight="2"
            android:textSize="@dimen/text_size_12"/>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#DCDCDC"/>
    </TableRow>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>

    <TableRow>

        <LinearLayout
            android:layout_span="9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="right"
            >
            <Button
                android:id="@+id/submit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:layout_margin="1dp"
                android:layout_span="1"
                android:gravity="center"
                android:minHeight="0dp"
                android:text="确认"
                android:textSize="@dimen/text_size_12"/>
            <Button
                android:id="@+id/cancel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:layout_margin="1dp"
                android:layout_span="1"
                android:gravity="center"
                android:minHeight="0dp"
                android:text="取消"
                android:textSize="@dimen/text_size_12"/>


        </LinearLayout>
    </TableRow>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>
</TableLayout>