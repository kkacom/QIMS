<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowBackground">@color/windowBackground</item>
    </style>
    <!-- 自定义对话框-->
    <style name="MyDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/round_rect_translucent_black_shape</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="Input">
        <item name="android:gravity">center</item>
        <item name="android:background">#F0F0F0</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <!--<item name="android:layout_height">@dimen/et_height</item>-->
        <item name="android:textSize">@dimen/text_size_10</item>
        <!--<item name="android:ems">4</item>-->
        <item name="android:layout_marginLeft">1.5dp</item>
    </style>
    <style name="VerticalTH">
        <item name="android:layout_marginLeft">2dp</item>
        <item name="android:background">#DCDCDC</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">@dimen/text_size_10</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="HorizontalTH">
        <item name="android:layout_marginLeft">1dp</item>
        <item name="android:background">#DCDCDC</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">@dimen/text_size_10</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="SmallTH">
        <item name="android:layout_marginLeft">1dp</item>
        <item name="android:background">#DCDCDC</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">@dimen/text_size_10</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="TableTitle">
        <item name="android:layout_width">0dp</item>
        <item name="android:textColor">@color/table_title_blue</item>
        <item name="android:layout_height">@dimen/row_height</item>
        <item name="android:textSize">@dimen/text_size_12</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="TableTitleT1">
        <item name="android:layout_marginLeft">3dp</item>
    <item name="android:layout_width">@dimen/col_width</item>
    <item name="android:textColor">@color/table_title_blue</item>
    <item name="android:layout_height">@dimen/row_height</item>
    <item name="android:textSize">@dimen/text_size_12</item>
    <item name="android:gravity">center</item>
</style>
    <style name="TableTitleL">
        <item name="android:layout_marginLeft">3dp</item>
        <item name="android:layout_width">@dimen/col_width</item>
        <item name="android:textColor">@color/table_title_blue</item>
        <item name="android:layout_height">@dimen/row_height</item>
        <item name="android:textSize">@dimen/text_size_12</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="TestInput">
        <item name="android:layout_marginLeft">3dp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_height">@dimen/row_height</item>
        <item name="android:textSize">@dimen/text_size_12</item>
        <item name="android:layout_width">@dimen/col_width</item>
        <item name="android:inputType">number</item>
        <!-- <item name="android:ems">4</item>-->
        <item name="android:background">@drawable/edit_bg</item>
    </style>
    <style name="TestInput1">
        <item name="android:layout_marginLeft">3dp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_height">@dimen/row_height</item>
        <item name="android:textSize">@dimen/text_size_12</item>
        <item name="android:layout_width">35dp</item>
        <item name="android:inputType">number</item>
        <!-- <item name="android:ems">4</item>-->
        <item name="android:background">@drawable/edit_bg</item>
    </style>
</resources>
