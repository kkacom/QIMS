<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
             android:layout_width="match_parent"
             android:layout_height="match_parent">

    <TableRow android:background="#6DEDD7">

        <TextView
            style="@style/TableTitle"
            android:layout_width="65dp"
            android:layout_height="match_parent"
            android:text="漆膜厚度\n(μm)"
            />

        <TableLayout>

            <TableRow>

                <TextView
                    style="@style/TableTitle"
                    android:text="左正"/>

                <TextView
                    style="@style/TableTitle"
                    android:text="左正"/>

                <TextView
                    style="@style/TableTitle"
                    android:text="左正"/>

                <TextView
                    style="@style/TableTitle"
                    android:text="左正"/>

            </TableRow>
            <TableRow>

                <EditText  android:id="@id/film_thickness_left_front1"
                           style="@style/TestInput"/>
                <EditText  android:id="@id/film_thickness_left_front2"
                           style="@style/TestInput"/>
                <EditText  android:id="@id/film_thickness_left_front3"
                           style="@style/TestInput"/>
                <EditText  android:id="@id/film_thickness_left_front4"
                           style="@style/TestInput"/>
            </TableRow>

            <TableRow>

                <TextView
                    style="@style/TableTitle"

                    android:text="左背"/>

                <TextView
                    style="@style/TableTitle"

                    android:text="左背"/>

                <TextView
                    style="@style/TableTitle"
                    android:text="左背"/>

            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <EditText  android:id="@id/film_thickness_left_back1"
                           style="@style/TestInput"/>

                <EditText  android:id="@id/film_thickness_left_back2"
                           style="@style/TestInput"/>

                <EditText  android:id="@id/film_thickness_left_back3"
                           style="@style/TestInput"/>

            </TableRow>

            <TableRow>

                <TextView
                    style="@style/TableTitle"
                    android:text="右正"/>

                <TextView
                    style="@style/TableTitle"
                    android:text="右正"/>

                <TextView
                    style="@style/TableTitle"
                    android:text="右正"/>

                <TextView
                    style="@style/TableTitle"
                    android:text="右正"/>
            </TableRow>
            <TableRow>

                <EditText  android:id="@id/film_thickness_right_front1"
                           style="@style/TestInput"/>
                <EditText  android:id="@id/film_thickness_right_front2"
                           style="@style/TestInput"/>
                <EditText  android:id="@id/film_thickness_right_front3"
                           style="@style/TestInput"/>
                <EditText  android:id="@id/film_thickness_right_front4"
                           style="@style/TestInput"/>
            </TableRow>

            <TableRow>

                <TextView
                    style="@style/TableTitle"

                    android:text="右背"/>

                <TextView
                    style="@style/TableTitle"

                    android:text="右背"/>

                <TextView
                    style="@style/TableTitle"
                    android:text="右背"/>

            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <EditText  android:id="@id/film_thickness_right_back1"
                           style="@style/TestInput"/>

                <EditText  android:id="@id/film_thickness_right_back2"
                           style="@style/TestInput"/>

                <EditText  android:id="@id/film_thickness_right_back3"
                           style="@style/TestInput"/>

            </TableRow>

            <TableRow android:layout_marginTop="5dp">
                <EditText android:id="@id/film_thickness_instruction"
                          style="@style/TestInput"
                          android:layout_span="4"
                          android:gravity="left|center"
                          android:hint="现场调查及说明"/>
            </TableRow>
        </TableLayout>
    </TableRow>
    <TableRow android:background="#EDD185" android:layout_marginTop="1dp">

        <TextView
            style="@style/TableTitle"
            android:layout_width="65dp"
            android:layout_height="match_parent"
            android:text="漆膜附着力(%)"/>

        <TableLayout>

            <TableRow>

                <TextView
                    style="@style/TableTitle"
                    android:text="左正"/>

                <TextView
                    style="@style/TableTitle"
                    android:text="左正"/>

                <TextView
                    style="@style/TableTitle"
                    android:text="左正"/>

                <TextView
                    style="@style/TableTitle"
                    android:text="左正"/>

            </TableRow>
            <TableRow>

                <EditText  android:id="@id/film_adhesion_left_front1"
                           style="@style/TestInput"/>
                <EditText  android:id="@id/film_adhesion_left_front2"
                           style="@style/TestInput"/>
                <EditText  android:id="@id/film_adhesion_left_front3"
                           style="@style/TestInput"/>
                <EditText  android:id="@id/film_adhesion_left_front4"
                           style="@style/TestInput"/>
            </TableRow>

            <TableRow>

                <TextView
                    style="@style/TableTitle"

                    android:text="左背"/>

                <TextView
                    style="@style/TableTitle"

                    android:text="左背"/>

                <TextView
                    style="@style/TableTitle"
                    android:text="左背"/>

            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <EditText  android:id="@id/film_adhesion_left_back1"
                           style="@style/TestInput"/>

                <EditText  android:id="@id/film_adhesion_left_back2"
                           style="@style/TestInput"/>

                <EditText  android:id="@id/film_adhesion_left_back3"
                           style="@style/TestInput"/>

            </TableRow>

            <TableRow>

                <TextView
                    style="@style/TableTitle"
                    android:text="右正"/>

                <TextView
                    style="@style/TableTitle"
                    android:text="右正"/>

                <TextView
                    style="@style/TableTitle"
                    android:text="右正"/>

                <TextView
                    style="@style/TableTitle"
                    android:text="右正"/>
            </TableRow>
            <TableRow>

                <EditText  android:id="@id/film_adhesion_right_front1"
                           style="@style/TestInput"/>
                <EditText  android:id="@id/film_adhesion_right_front2"
                           style="@style/TestInput"/>
                <EditText  android:id="@id/film_adhesion_right_front3"
                           style="@style/TestInput"/>
                <EditText  android:id="@id/film_adhesion_right_front4"
                           style="@style/TestInput"/>
            </TableRow>

            <TableRow>

                <TextView
                    style="@style/TableTitle"

                    android:text="右背"/>

                <TextView
                    style="@style/TableTitle"

                    android:text="右背"/>

                <TextView
                    style="@style/TableTitle"
                    android:text="右背"/>

            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <EditText  android:id="@id/film_adhesion_right_back1"
                           style="@style/TestInput"/>

                <EditText  android:id="@id/film_adhesion_right_back2"
                           style="@style/TestInput"/>

                <EditText  android:id="@id/film_adhesion_right_back3"
                           style="@style/TestInput"/>

            </TableRow>

            <TableRow android:layout_marginTop="5dp">
                <EditText android:id="@id/film_adhesion_instruction"
                          style="@style/TestInput"
                          android:layout_span="4"
                          android:gravity="left|center"
                          android:hint="现场调查及说明"/>
            </TableRow>
        </TableLayout>
    </TableRow>
</TableLayout>