apply plugin: 'com.android.application'
apply plugin: 'org.greenrobot.greendao'
android {
    signingConfigs {
    }
    compileSdkVersion 26
    defaultConfig {
        multiDexEnabled true
        applicationId "com.zhilian.rf_qims"
        minSdkVersion 21
        targetSdkVersion 26
        versionCode 1
        versionName "1.0"
    }
    buildTypes {
        release {
            minifyEnabled false
            proguardFiles getDefaultProguardFile('proguard-android.txt'), 'proguard-rules.pro'
        }
    }
    /*针对greenDao的一些配置*/
    greendao {
        schemaVersion 1//数据库版本号
        daoPackage 'com.zhilian.rf_qims.dao'//自动生成的工具类的包名
        targetGenDir "src/main/java"//路径
    }
    compileOptions {
        targetCompatibility JavaVersion.VERSION_1_8
        sourceCompatibility JavaVersion.VERSION_1_8
    }
    buildToolsVersion '27.0.3'
    useLibrary 'org.apache.http.legacy'
}


dependencies {
    compile fileTree(include: ['*.jar'], dir: 'libs')
    //noinspection GradleCompatible
    implementation deps.appcompat
    testCompile 'junit:junit:4.12'
    compile project(':colin-utils')
    //第三方依赖库
    //implementation deps.kotlin
    compile deps.butterknife
    annotationProcessor deps.butterknifeCompiler
    implementation deps.appcompat
    implementation deps.qmui
    implementation deps.junit
    implementation deps.loggingInterceptor
    implementation deps.okhttp
    implementation deps.gson
    implementation deps.volley
    implementation deps.gradientui
    implementation deps.rxJavaAdapter
    implementation deps.gosnConverter
    implementation deps.retrofit
    implementation deps.rxAndroid
    implementation deps.rxJava
    implementation deps.disklruCache
    implementation deps.easypermission
    implementation deps.pdfView
    implementation deps.swipeMenuListView
    implementation deps.rxPermissions
    implementation deps.greendao
    implementation deps.constraint
    implementation 'com.bigkoo:pickerview:2.0.8'
    implementation 'com.github.venusic:E-signature:0.2'
    compile 'com.github.LuckSiege.PictureSelector:picture_library:v2.2.3'
    implementation 'com.zhy:okhttputils:2.3.9'
    compile('com.github.afollestad.material-dialogs:core:0.8.5.3@aar') {
        transitive = true
    }
    implementation files('libs/fastjson-1.1.53.android.jar')
}
repositories {
    maven { url "https://jitpack.io" }
}
