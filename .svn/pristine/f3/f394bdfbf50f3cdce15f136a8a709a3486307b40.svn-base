            android:padding="5dp">


            <TextView
                android:id="@+id/thm_jx_item1"
                style="@style/SmallTopTitle"

                android:layout_marginStart="8dp"
                android:text="左上"
                android:textSize="@dimen/text_size_12"
                app:layout_constraintStart_toEndOf="@+id/thm_jx_title"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/thm_jx_item2"
                style="@style/SmallTopTitle"
                android:layout_marginStart="8dp"
                android:text="左中"
                android:textSize="@dimen/text_size_12"
                app:layout_constraintBaseline_toBaselineOf="@+id/thm_jx_item1"
                app:layout_constraintStart_toEndOf="@+id/thm_jx_item1" />

            <TextView
