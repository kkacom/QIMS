                style="@style/SmallInputValue"
                android:layout_marginLeft="8dp"
                app:layout_constraintBaseline_toBaselineOf="@+id/thm_jtck3"
                app:layout_constraintStart_toEndOf="@+id/thm_jtck3" />

            <EditText
                android:id="@+id/thm_jtck5"
                style="@style/SmallInputValue"
                android:layout_marginLeft="8dp"
                app:layout_constraintBaseline_toBaselineOf="@+id/thm_jtck4"
                app:layout_constraintStart_toEndOf="@+id/thm_jtck4" />

            <EditText
                android:id="@+id/thm_jtck6"
