                android:layout_marginLeft="8dp"
                app:layout_constraintBaseline_toBaselineOf="@+id/thm_b_zxxpc3"
                app:layout_constraintStart_toEndOf="@+id/thm_b_zxxpc3" />

            <EditText
                android:id="@+id/thm_b_zxxpc5"
