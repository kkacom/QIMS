                app:layout_constraintStart_toEndOf="@+id/thm_jx_item2" />

            <TextView
                android:id="@+id/thm_jx_item4"
                style="@style/SmallTopTitle"
                android:layout_marginStart="8dp"
                android:text="右上"
                android:textSize="@dimen/text_size_12"
                app:layout_constraintBaseline_toBaselineOf="@+id/thm_jx_item3"
                app:layout_constraintStart_toEndOf="@+id/thm_jx_item3" />

            <TextView
                android:id="@+id/thm_jx_item5"
                style="@style/SmallTopTitle"
                android:layout_marginStart="8dp"
                android:text="右中"
                android:textSize="@dimen/text_size_12"
                app:layout_constraintBaseline_toBaselineOf="@+id/thm_jx_item4"
                app:layout_constraintStart_toEndOf="@+id/thm_jx_item4" />

            <TextView
                android:id="@+id/thm_jx_item6"
                style="@style/SmallTopTitle"
                android:layout_marginStart="8dp"
                android:text="右下"
                android:textSize="@dimen/text_size_12"
