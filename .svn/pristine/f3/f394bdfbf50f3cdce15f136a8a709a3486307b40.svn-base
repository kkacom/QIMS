                android:id="@+id/thm_jtck_bz"
                style="@style/SmallInputValue"
                android:layout_width="220dp"
                android:layout_marginTop="8dp"
                android:gravity="center_vertical"
                android:hint="现场调查及说明"

                app:layout_constraintStart_toStartOf="@+id/thm_qybkd1"
                app:layout_constraintTop_toBottomOf="@+id/thm_qybkd2" />
